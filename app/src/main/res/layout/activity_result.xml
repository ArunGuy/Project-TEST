<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resultVM" type="com.arunwichsapplication.app.modules.result.data.viewmodel.ResultVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleyellow_50">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearHead"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_52pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_indigo_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_indigo_300"/>
                    <androidx.appcompat.widget.SearchView
                        android:background="@drawable/rectangle_border_indigo_300_radius_20"
                        android:id="@+id/searchViewFrame2432"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:theme="@style/searchViewFrame2432"
                        app:iconifiedByDefault="false"
                        app:queryBackground="@android:color/transparent"
                        app:queryHint="@string/lbl_search" app:searchIcon="@drawable/img_search_gray_400"/>
                    <ImageView android:id="@+id/imageSearchOne"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_indigo_300_20x20"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_indigo_300_20x20"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowframe2433"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame2433"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.38"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{resultVM.resultModel.txtFrame2433}"
                    style="@style/txtSolid" tools:text="@string/lbl_relevance"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame2434"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.26"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{resultVM.resultModel.txtFrame2434}"
                    style="@style/txtSolid" tools:text="@string/lbl_latest"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame2435"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.36"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@{resultVM.resultModel.txtFrame2435}"
                    style="@style/txtSolid" tools:text="@string/lbl_top_sales"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowframe2441"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowcheckmark"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylegray_200cornerRadius">
                    <ImageView android:id="@+id/imageCheckmark"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_checkmark_red_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_red_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMall"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{resultVM.resultModel.txtMall}"
                        style="@style/txtIntersemibold16_3" tools:text="@string/lbl_mall"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.43"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylegray_201cornerRadius">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_teal_400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_400"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{resultVM.resultModel.txtLanguage}"
                        style="@style/txtIntersemibold16_3" tools:text="@string/lbl_free_shipping"/>
                </LinearLayout>
                <Spinner
                    android:background="@drawable/layer_list_bg_gray_201_radius_10"
                    android:id="@+id/spinnerFrame2441"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.38"
                    android:layout_width="0dp" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearResult"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleyellow_50">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnfive"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleyellow_50">
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerResult"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_20pxh"
                    android:layout_marginVertical="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="2" itemSpace="@{@dimen/_20pxh}"
                    tools:itemCount="6" tools:listitem="@layout/row_result"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
